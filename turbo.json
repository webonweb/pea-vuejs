{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "pea-vuejs#build": {
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "@pea-vuejs/plugins#build": {
      "dependsOn": ["pea-vuejs#build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}

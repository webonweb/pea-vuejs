{
  "name": "pea-vuejs",
  "type": "module",
  "version": "0.1.0",
  "description": "Small UI library for non styled components.",
  "author": "Pea-VueJs Contributors (https://github.com/webonweb/pea-vuejs)",
  "license": "MIT",
  "homepage": "https://github.com/webonweb/pea-vuejs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webonweb/pea-vuejs.git"
  },
  "bugs": {
    "url": "https://github.com/webonweb/pea-vuejs/issues"
  },
  "keywords": [
    "ui",
    "components",
    "accessibility",
    "seo",
    "fast",
    "small",
    "styled",
    "unstyled",
    "headless",
    "vue",
    "nuxt",
    "primitives",
    "pea",
    "vuejs",
    "unstyled",
    "pea-vuejs"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.umd.cjs",
      "import": "./dist/index.js"
    },
    "./headless": {
      "types": "./dist/headless.d.ts",
      "require": "./dist/headless.umd.cjs",
      "import": "./dist/headless.js"
    },
    "./nuxt": {
      "require": {
        "default": "./dist/nuxt/index.cjs",
        "types": "./dist/nuxt/index.d.cts"
      },
      "import": {
        "default": "./dist/nuxt/index.mjs",
        "types": "./dist/nuxt/index.d.mts"
      }
    }
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "./dist",
    "./LICENSE"
  ],
  "scripts": {
    "build": "bun type-check && bun build-only",
    "build-only": "vite build",
    "watch": "vite build --watch",
    "type-check": "vue-tsc --noEmit",
    "type-gen": "vue-tsc --declaration  --emitDeclarationOnly",
    "test": "vitest",
    "pub:release": "npm publish --access public",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build -o '.output'"
  },
  "peerDependencies": {
    "vue": ">= 3.2.0"
  },
  "devDependencies": {
    "storybook": "^8.1.1",
    "@storybook/addon-docs": "^8.1.1",
    "@storybook/addon-essentials": "^8.1.1",
    "@storybook/addon-interactions": "^8.1.1",
    "@storybook/addon-links": "^8.1.1",
    "@storybook/blocks": "^8.1.1",
    "@storybook/core-common": "^8.1.1",
    "@storybook/csf-tools": "^8.1.1",
    "@storybook/jest": "0.2.3",
    "@storybook/node-logger": "^8.1.1",
    "@storybook/test-runner": "0.17.0",
    "@storybook/testing-library": "0.2.2",
    "@storybook/vue3": "^8.1.1",
    "@storybook/vue3-vite": "^8.1.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/user-event": "^14.5.1",
    "@testing-library/vue": "^7.0.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.18.6",
    "vite-svg-loader": "^5.1.0",
    "vite-plugin-glob": "^0.3.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/shared": "^3.4.6",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "jsdom": "^23.2.0",
    "typescript": "^5.3.3",
    "vite": "^4.5.3",
    "vitest": "^1.5.2",
    "vite-plugin-dts": "^3.6.0",
    "vue": "^3.4.19",
    "vue-tsc": "1.8.27",
    "@pea-vuejs/primitive": "workspace:*",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "sass": "^1.77.1",
    "tailwindcss": "^3.4.3",
    "simple-code-editor": "^2.0.9"
  }
}
